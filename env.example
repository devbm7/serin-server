# Supabase Configuration
# Copy this file to .env and fill in your actual values

# Supabase URL (required)
SUPABASE_URL=https://your-project.supabase.co
# or use the same as your Next.js app
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co

# Supabase Service Role Key (preferred for server-side operations)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Supabase Anon Key (fallback, less secure for server operations)
SUPABASE_ANON_KEY=your-anon-key-here
# or use the same as your Next.js app
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# FastAPI Server Configuration
FASTAPI_HOST=0.0.0.0
FASTAPI_PORT=8000
FASTAPI_RELOAD=false

# Gemini API Key (if using Gemini LLM)
GEMINI_API_KEY=your-gemini-api-key-here
